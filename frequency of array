//frequency of array
#include<stdio.h>
int main()
{
	int a[1000];
	int n,i,j,count=0;
	printf("enter the size of the array:");
	scanf("%d",&n);
	printf("enter the elements of the array:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
	for(j=0;j<n;j++)
	{
		if(a[i]==a[j]){
			count=count+1;
		}
		else{
			count=count+0;
		}
		
	}

	

printf("\nthe frequency of %d is %d",a[i],count);
count=0;
}
}
