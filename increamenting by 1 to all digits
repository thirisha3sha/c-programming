//increament by 1 to all didgits

#include <stdio.h>

int main() {
    int n,sum,remainder;
    printf("enter the number;");
    scanf("%d",&n);
    sum=0;
    while(n!=0){
        remainder=n%10;
        sum=sum+(remainder+1);
        n=n/10;
    }
    printf("increasing 1 to all digits is %d",sum);

    return 0;
}
