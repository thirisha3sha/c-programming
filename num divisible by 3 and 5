//divisible by 3 and 5

#include <stdio.h>

int main() {
    int num;
    printf("enter the number:");
    scanf("%d",&num);
     if(num%3==0 && num%5==0){
        printf("%d is divible by both 3 and 5",num);
     }
    
    else if(num%3==0){
        printf("%d is divisible by 3",num);
        
    }
    else if(num%5==0){
        printf("%d is divisible by 5",num);
    }
    
    else{
        printf("%d is not divisible 3 and 5",num);
    }
    return 0;
}

